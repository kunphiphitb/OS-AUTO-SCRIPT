client
dev tun
dev-type tun
proto tcp
remote-random
http-proxy localhost 8989

<connection>
remote open-serviberth.duckdns.org:443@anywhere.truevisions.tv 443
</connection>

<connection>
remote open-serviberth.duckdns.org:1194@anywhere.truevisions.tv 1194
</connection>

<connection>
remote open-serviberth.duckdns.org:5002@anywhere.truevisions.tv 5002
</connection>

resolv-retry infinite
resolv-retry 86400
ifconfig-nowarn
persist-tun
persist-key
tun-mtu 1500
mssfix 1450
nobind
route 0.0.0.0 0.0.0.0 net_gateway
route 10.0.0.0 255.255.255.0 net_gateway
route 172.16.0.0 255.255.255.0 net_gateway
route 192.168.0.0 255.255.255.0 net_gateway
route 10.10.10.10 255.255.255.0 net_gateway
route 203.144.0.0 255.255.255.0 net_gateway
route 203.150.0.0 255.255.255.0 net_gateway
route 61.91.0.0 255.255.255.0 net_gateway
cipher AES-128-CBC
digest SHA1
ping 5
ping-restart 60
mute-replay-warnings
push-peer-info
connect-retry-max 5
connect-retry 5
auth-user-pass
register-dns
float
verb 4
